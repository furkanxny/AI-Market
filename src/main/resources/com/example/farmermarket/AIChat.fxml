<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.farmermarket.AIChatController">
   <top>
      <Pane prefHeight="98.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" layoutX="158.0" layoutY="14.0" prefHeight="73.0" prefWidth="163.0" text="Prompt" textFill="#224a8f">
               <font>
                  <Font size="52.0" />
               </font>
            </Label>
            <TextField fx:id="prompt" layoutX="372.0" layoutY="-14.0" prefHeight="119.0" prefWidth="574.0" promptText="Enter your prompt">
               <font>
                  <Font size="63.0" />
               </font>
            </TextField>
         </children>
      </Pane>
   </top>
   <bottom>
      <Label alignment="CENTER" prefHeight="123.0" prefWidth="575.0" text="AI CHAT" textFill="#7e2fa1" BorderPane.alignment="CENTER">
         <font>
            <Font name="Impact" size="96.0" />
         </font>
      </Label>
   </bottom>
   <center>
      <TextArea fx:id="output" prefHeight="433.0" prefWidth="1125.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <Pane prefHeight="477.0" prefWidth="75.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="enter" layoutX="-1.0" layoutY="143.0" mnemonicParsing="false" onAction="#enterOnAction" prefHeight="46.0" prefWidth="77.0" text="Enter" />
         </children>
      </Pane>
   </right>
   <left>
      <Pane prefHeight="579.0" prefWidth="120.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
